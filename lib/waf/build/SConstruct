env = Environment(CPPFLAGS=['-Wall'], CPPDEFINES=['LINUX'], CPPPATH=[Dir('#')])
env.Decider('timestamp-newer')
env.SetOption('implicit_cache', True)
env.SourceCode('.', None)
env.SConscript("lib_0/SConscript", exports=['env'])
env.SConscript("lib_1/SConscript", exports=['env'])
